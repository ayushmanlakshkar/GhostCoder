# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# GitHub Personal Access Token (needs repo and PR permissions)
GITHUB_TOKEN=your_github_token_here

# Optional: Custom temporary directory for cloning repos
TEMP_DIR=./temp_repos

# Optional: OpenRouter model to use
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet

# Optional: Base branch name for PRs
BASE_BRANCH=ghostcoder-improvements
